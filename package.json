{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "UMI_ENV=cloud umi build",
    "release": "git checkout -b release-1.0.0 & npm run build",
    "copy": "rm -rf src mock *.js *.ts *.md *.yml & cp dist/* ./",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@formatjs/intl": "^1.11.1",
    "@umijs/preset-react": "1.x",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "gg-editor": "^2.0.4",
    "immutability-helper": "^3.1.1",
    "mobx": "^6.3.2",
    "mobx-react": "^7.2.0",
    "mockjs": "^1.1.0",
    "react-dnd": "^14.0.2",
    "react-dnd-html5-backend": "^14.0.0",
    "react-intl": "^5.18.1",
    "umi": "^3.4.4"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/test": "^3.4.4",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "^17.0.2",
    "react-dom": "17.x",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
