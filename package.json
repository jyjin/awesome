{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "UMI_ENV=cloud umi build",
    "release": "git checkout -b release-1.0.4 & npm run build",
    "copy": "rm -rf src mock config *.js *.ts *.md *.yml & cp dist/* ./",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "jest": "jest",
    "test": "umi-test --watch",
    "test:coverage": "umi-test  --watch --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@ckeditor/ckeditor5-build-classic": "^34.0.0",
    "@ckeditor/ckeditor5-core": "^34.0.0",
    "@ckeditor/ckeditor5-image": "^34.0.0",
    "@ckeditor/ckeditor5-react": "^5.0.0",
    "@ckeditor/ckeditor5-ui": "^34.0.0",
    "@formatjs/intl": "^1.11.1",
    "@uiw/react-textarea-code-editor": "^1.3.1",
    "@umijs/preset-react": "1.x",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "enzyme": "^3.11.0",
    "gg-editor": "^2.0.4",
    "heic2any": "0.0.3",
    "i": "^0.3.6",
    "immutability-helper": "^3.1.1",
    "lodash": "^4.17.21",
    "mobx": "^6.3.2",
    "mobx-react": "^7.2.0",
    "mockjs": "^1.1.0",
    "npm": "^7.20.6",
    "parchment": "^2.0.0-dev.2",
    "qrcode.react": "^3.0.1",
    "quill": "^1.3.7",
    "quill-delta-to-html": "^0.12.1",
    "react-color": "^2.19.3",
    "react-cropper": "^2.1.8",
    "react-dnd": "^14.0.2",
    "react-dnd-html5-backend": "^14.0.0",
    "react-intl": "^5.18.1",
    "react-quill": "^1.3.5",
    "react-sortable": "^2.0.0",
    "react-sortablejs": "^6.0.0",
    "sortablejs": "^1.14.0",
    "umi": "^3.4.4",
    "uuid": "^8.3.2",
    "vconsole": "^3.12.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/sortablejs": "^1.10.7",
    "@umijs/test": "^3.4.4",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "babel-preset-react-app": "^10.0.0",
    "jest": "^27.0.6",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "^17.0.2",
    "react-dom": "17.x",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
